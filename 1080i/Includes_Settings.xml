                        <include content="Object_TextColor" condition="$PARAM[dialog]">
                            <param name="textcolor">dialog_fg_70</param>
                        </include>
                        <scroll>true</scroll>
                    </include>
                    <include content="Object_InfoCircle_Text_Sub" condition="$PARAM[override]">
