<?xml version="1.0" encoding="UTF-8"?>
<includes>

    <include name="Generator_BuildTextures_Onload">
        <description>Null</description>
    </include>

    <!-- Texture Maker -->
    <include name="Texture_Highlight">
        <texture diffuse="$PARAM[diffuse]" colordiffuse="$VAR[ColorHighlight]">common/main.png</texture>
    </include>
    <include name="Texture_Highlight_Focus">
        <texturefocus colordiffuse="$VAR[ColorHighlight]">common/main.png</texturefocus>
        <alttexturefocus colordiffuse="$VAR[ColorHighlight]">common/main.png</alttexturefocus>
    </include>
    <include name="Texture_Highlight_ToggleButton_FakeFocus">
        <texturenofocus />
        <texturefocus colordiffuse="$VAR[ColorHighlight]" border="40">common/menu.png</texturefocus>
        <alttexturefocus colordiffuse="$VAR[ColorHighlight]" border="40">common/menu.png</alttexturefocus>
        <alttexturenofocus colordiffuse="$VAR[ColorHighlight]" border="40">common/menu.png</alttexturenofocus>
    </include>
    <include name="Texture_Highlight_ToggleButton_Settings">
        <texturefocus colordiffuse="$VAR[ColorSettingsToggleButton_Native]" border="40">common/menu.png</texturefocus>
        <alttexturefocus colordiffuse="$VAR[ColorSettingsToggleButton_Native]" border="40">common/menu.png</alttexturefocus>
    </include>
    <include name="Texture_Highlight_Underline_H">
        <texturefocus border="4" colordiffuse="$VAR[ColorHighlight]">common/underline.png</texturefocus>
        <alttexturefocus border="4" colordiffuse="$VAR[ColorHighlight]">common/underline.png</alttexturefocus>
        <alttexturenofocus border="4" colordiffuse="$VAR[ColorHighlight]">common/underline.png</alttexturenofocus>
        <texturenofocus />
    </include>
    <include name="Texture_Highlight_Scrollbar_H">
        <texture border="4" colordiffuse="$VAR[ColorHighlight]">common/scrollh.png</texture>
    </include>
    <include name="Texture_Scrollbar_H">
        <texturesliderbarfocus border="4" colordiffuse="$VAR[ColorHighlight]">common/scrollh.png</texturesliderbarfocus>
    </include>
    <include name="Texture_Scrollbar_SliderBar_H">
        <texturesliderbar border="4" colordiffuse="$VAR[ColorHighlight]">common/scrollh.png</texturesliderbar>
    </include>
    <include name="Texture_Scrollbar_V">
        <texturesliderbarfocus border="4" colordiffuse="$VAR[ColorHighlight]">common/scrollv.png</texturesliderbarfocus>
    </include>
    <include name="Texture_Scrollbar_SliderBar_V">
        <texturesliderbar border="4" colordiffuse="$VAR[ColorHighlight]">common/scrollv.png</texturesliderbar>
    </include>
    <include name="Texture_MainMenu_Highlight_H">
        <texture border="80" colordiffuse="$VAR[ColorHighlight]">common/menumain.png</texture>
    </include>
    <include name="Texture_MainMenu_Highlight_H_X">
        <texture border="80,0,0,0" colordiffuse="$VAR[ColorHighlight]">common/menumain_x.png</texture>
    </include>
    <include name="Texture_MainMenu_Highlight_Focus_H_Y">
        <texturefocus colordiffuse="$VAR[ColorHighlight]">common/menumain_y.png</texturefocus>
        <alttexturefocus colordiffuse="$VAR[ColorHighlight]">common/menumain_y.png</alttexturefocus>
    </include>
    <include name="Texture_MainMenu_Highlight_H_Z">
        <texture border="0,0,80,0" colordiffuse="$VAR[ColorHighlight]">common/menumain_z.png</texture>
    </include>
    <include name="Texture_Box_Highlight_H">
        <texture border="12" colordiffuse="$VAR[ColorHighlight]">common/box.png</texture>
    </include>
    <include name="Texture_Box_Highlight_V">
        <texture border="12" colordiffuse="$VAR[ColorHighlight]">common/box.png</texture>
    </include>
    <include name="Texture_Box_Highlight_Focus_H">
        <texturefocus border="12" colordiffuse="$VAR[ColorHighlight]">common/box.png</texturefocus>
    </include>
    <include name="Texture_Menu_Highlight_H">
        <param name="border">40</param>
        <definition>
            <texture border="$PARAM[border]" colordiffuse="$VAR[ColorHighlight]">common/menu.png</texture>
        </definition>
    </include>
    <include name="Texture_Circle_Highlight_H">
        <texture colordiffuse="$VAR[ColorHighlight]">common/circle.png</texture>
    </include>
    <include name="Texture_Circle_Highlight_V">
        <texture flipy="true" colordiffuse="$VAR[ColorHighlight]">common/circle.png</texture>
    </include>
    <include name="Texture_Circle_Highlight_Focus_V">
        <texturefocus flipy="true" colordiffuse="$VAR[ColorHighlight]">common/circle.png</texturefocus>
    </include>
    <include name="Texture_Circle_Highlight_NoFocus_V">
        <texturenofocus flipy="true" colordiffuse="$VAR[ColorHighlight]">common/circle.png</texturenofocus>
    </include>
    <include name="Texture_CircleButtonDialog_Highlight_V">
        <texture flipy="true" colordiffuse="$VAR[ColorHighlight]">common/circlebutton.png</texture>
    </include>
    <include name="Texture_CircleButtonDialog_Highlight_Focus_V">
        <texturefocus flipy="true" colordiffuse="$VAR[ColorHighlight]">common/circlebutton.png</texturefocus>
    </include>
    <include name="Texture_SelectBox_Thin">
        <texture border="20" colordiffuse="$VAR[ColorHighlight]">common/selectbox_thin.png</texture>
    </include>
    <include name="Texture_KeyboardButton">
        <texturefocus border="40" colordiffuse="$VAR[ColorHighlight]">common/circle.png</texturefocus>
    </include>
    <include name="Texture_Button">
        <texturenofocus colordiffuse="main_fg_12" border="40">common/menu.png</texturenofocus>
        <texturefocus border="40" colordiffuse="$VAR[ColorHighlight]">common/menu.png</texturefocus>
    </include>
    <include name="Texture_ToggleButton">
        <alttexturenofocus colordiffuse="main_bg_12" border="40">common/menu.png</alttexturenofocus>
        <alttexturefocus border="40" colordiffuse="$VAR[ColorHighlight]">common/menu.png</alttexturefocus>
    </include>

    <include name="Defs_BusySpinner_Image">
        <param name="spinspinner">true</param>
        <definition>
            <description>Busy animation</description>
            <texture colordiffuse="$VAR[ColorHighlight]">dialog/loading-spinner.png</texture>
            <aspectratio>keep</aspectratio>
            <animation effect="rotate" condition="$PARAM[spinspinner]" start="0" end="360" loop="true" time="1000" center="auto">Conditional</animation>
        </definition>
    </include>

</includes>
