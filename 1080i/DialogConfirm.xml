<?xml version="1.0" encoding="UTF-8"?>
<window>
    <onunload condition="!String.IsEmpty(Window(Home).Property(StartupReplaceWindow))">ActivateWindow(1198)</onunload>
    <controls>
        <control type="image">
            <include>Animation_FadeIn</include>
            <include>Animation_FadeOut</include>
            <texture colordiffuse="panel_bg">common/white.png</texture>
        </control>
        <control type="group">
            <include>Animation_SlideIn_Dialog</include>
            <!-- <include content="Object_Dim_Bottom">
                <param name="flipy">false</param>
                <left>-120</left>
                <right>-120</right>
                <height>1040</height>
                <bottom>-320</bottom>
            </include> -->
            <control type="group">
                <include>Dimension_DialogConfirm</include>
                <include content="Dialog_Background_Blur">
                    <param name="diffuse">diffuse/dialog_w1120_h400.png</param>
                    <include content="Dialog_Head">
                        <top>40</top>
                        <left>40</left>
                    </include>
                    <control type="group">
                        <control type="image">
                            <bottom>120</bottom>
                            <texture colordiffuse="dialog_fg_12" border="20" flipy="true">common/dialog_overlay_v.png</texture>
                        </control>
                        <control type="image">
                            <top>120</top>
                            <height>1</height>
                            <texture colordiffuse="dialog_fg_12">common/white.png</texture>
                            <left>40</left>
                            <right>40</right>
                        </control>
                        <include content="Dialog_Group">
                            <top>160</top>
                            <bottom>160</bottom>
                            <include content="Dialog_Textbox">
                                <top>-8</top>
                                <bottom>8</bottom>
                            </include>
                            <control type="group">
                                <bottom>0</bottom>
                                <height>12</height>
                                <control type="image">
                                    <height>1</height>
                                    <centertop>50%</centertop>
                                    <visible>Control.IsVisible(20)</visible>
                                    <texture colordiffuse="dialog_fg_12">common/white.png</texture>
                                </control>
                                <control type="progress" id="20">
                                    <description>Progressbar</description>
                                    <info>System.Progressbar</info>
                                    <texturebg colordiffuse="00ffffff" border="4">progress/progress-bg.png</texturebg>
                                </control>
                                <control type="progress">
                                    <description>Progressbar</description>
                                    <info>System.Progressbar</info>
                                    <texturebg colordiffuse="00ffffff" border="4">progress/progress-bg.png</texturebg>
                                    <lefttexture colordiffuse="00ffffff" border="4">progress/progress-left.png</lefttexture>
                                    <midtexture colordiffuse="$VAR[ColorGradient]" border="4" diffuse="progress/diffuse.png">progress/progress-end.png</midtexture>
                                    <righttexture colordiffuse="$VAR[ColorGradient]" border="4" diffuse="progress/diffuse-end.png">progress/progress-right.png</righttexture>
                                    <include content="Defs_Progress_Colorfade">
                                        <param name="id">20</param>
                                    </include>
                                </control>
                            </control>
                        </include>
                    </control>

                    <control type="grouplist">
                        <right>40</right>
                        <left>40</left>
                        <height>80</height>
                        <bottom>20</bottom>
                        <align>center</align>
                        <orientation>horizontal</orientation>
                        <itemgap>20</itemgap>
                        <control type="button" id="12">
                            <width>280</width>
                            <visible>Control.IsEnabled(12)</visible>
                            <include>defs_dialog_button</include>
                        </control>
                        <control type="button" id="11">
                            <width>280</width>
                            <visible>Control.IsEnabled(11)</visible>
                            <label>107</label>
                            <include>defs_dialog_button</include>
                        </control>
                        <control type="button" id="10">
                            <width>280</width>
                            <visible>Control.IsEnabled(10)</visible>
                            <label>106</label>
                            <include>defs_dialog_button</include>
                        </control>
                    </control>
                </include>
            </control>
        </control>
    </controls>
</window>
