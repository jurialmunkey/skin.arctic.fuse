<?xml version="1.0" encoding="UTF-8"?>
<window type="dialog" id="1115">
    <defaultcontrol always="true">8001</defaultcontrol>
    <onunload>ClearProperty(CustomDialogSettingsStyle,Home)</onunload>
    <onunload>ClearProperty(CustomDialogSettingsImage,Home)</onunload>
    <onunload>ClearProperty(CustomDialogSettingsLabel,Home)</onunload>
    <controls>

        <include content="Dialog_Overlay_Side">
            <param name="topline">false</param>
            <param name="subline">false</param>
            <include>Animation_Right_Delay</include>
            <include>Dimension_DialogFullMenu</include>
            <include content="Dialog_Head">
                <param name="id">7000</param>
                <param name="label">$INFO[Window(Home).Property(CustomDialogSettingsTitle)]</param>
            </include>
            <include content="Dialog_Body">

                <include content="Dialog_Body_Group">
                    <width>800</width>
                    <control type="group">
                        <top>20</top>
                        <left>20</left>
                        <bottom>20</bottom>
                        <width>720</width>
                        <control type="image">
                            <top>-40</top>
                            <height>1</height>
                            <texture colordiffuse="dialog_fg_12">common/white.png</texture>
                        </control>
                        <control type="image">
                            <bottom>-40</bottom>
                            <height>1</height>
                            <texture colordiffuse="dialog_fg_12">common/white.png</texture>
                        </control>
                        <control type="group">
                            <height>405</height>
                            <control type="image">
                                <bottom>-40</bottom>
                                <height>1</height>
                                <texture colordiffuse="dialog_fg_12">common/white.png</texture>
                            </control>
                            <include content="Dialog_Outline_Box">
                                <control type="image">
                                    <width>128</width>
                                    <height>128</height>
                                    <centertop>50%</centertop>
                                    <centerleft>50%</centerleft>
                                    <texture colordiffuse="main_fg_90">special://skin/extras/icons/image.png</texture>
                                </control>
                                <control type="image">
                                    <aspectratio scalediffuse="false">scale</aspectratio>
                                    <texture background="true" diffuse="diffuse/landscape_w720_h405.png">$INFO[Window(Home).Property(CustomDialogSettingsImage)]</texture>
                                </control>
                            </include>
                        </control>
                        <control type="group">
                            <top>405</top>
                            <control type="textbox">
                                <top>65</top>
                                <bottom>-15</bottom>
                                <font>font_mini_plot</font>
                                <label>$INFO[Window(Home).Property(CustomDialogSettingsLabel)]</label>
                            </control>
                        </control>
                    </control>

                </include>

                <control type="group">
                    <right>160</right>
                    <left>800</left>
                    <control type="image">
                        <texture colordiffuse="dialog_fg_12" border="20">common/dialog.png</texture>
                    </control>
                    <include content="Dialog_Body_Grouplist">
                        <param name="id">8000</param>
                        <right>20</right>
                        <pagecontrol>61</pagecontrol>
                        <onleft>Close</onleft>
                        <onright>61</onright>
                        <include>Items_Settings_CustomDialog</include>
                    </include>
                </control>

                <include content="Dialog_Scrollbar">
                    <param name="id">61</param>
                    <onleft>8000</onleft>
                    <onright>9000</onright>
                </include>

                <include content="Dialog_Side_Grouplist">
                    <param name="id">9000</param>
                    <onleft>61</onleft>
                    <onright>Close</onright>
                    <include content="Dialog_Side_Button">
                        <param name="id">01</param>
                        <param name="label">$LOCALIZE[15067]</param>
                        <param name="icon">special://skin/extras/icons/circle-xmark.png</param>
                        <param name="visible">true</param>
                        <onclick>Close</onclick>
                    </include>
                </include>
            </include>
        </include>
    </controls>
</window>
