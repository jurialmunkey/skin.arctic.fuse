{
    "output": "template.xml",
    "header": "<?xml version=\"1.0\" encoding=\"UTF-8\"?>\n<template>",
    "footer": "</template>",
    "global": {
        "parts_stackedwidgets": {
            "template": "skinvariables-generator-parts-stackedwidgets.xmltemplate",
            "prev_id": "$PYTHON[5003 + int(id) - 1]"
        },
        "parts_standardwidgets": {
            "template": "skinvariables-generator-parts-standardwidgets.xmltemplate"
        },
        "parts_widgets": {
            "template": "skinvariables-generator-parts-widgets.xmltemplate",
            "id": "$PYTHON[5003 + int(id)]",
            "groupid": "$PYTHON[5103 + int(id)]",
            "grouplistid": "5000",
            "squareid": "5001",
            "landscapeid": "5002",
            "posterid": "5003",
            "topid": "5004",
            "refocus": "SetFocus(393)",
            "visible": "[true]",
            "altvisible": "[$SKINSHORTCUTS[widgetAltVisible]]",
            "include_condition": "$SKINSHORTCUTS[isVisible]"
        }
    },
    "genxml": [
        {
            "template": "skinvariables-generator-main.xmltemplate",
            "parts_stackedwidgets": {
                "template": "skinvariables-generator-parts-stackedwidgets.xmltemplate",
                "prev_id": "$PYTHON[int(mainmenuid) * 1000 + 600 + int(id) - 1]"
            },
            "parts_adaptivewidgets": {
                "template": "skinvariables-generator-parts-adaptivewidgets.xmltemplate",
                "landscapeid": "$PYTHON[int(mainmenuid) * 1000 + 411]",
                "landscapegroupid": "$PYTHON[int(mainmenuid) * 1000 + 412]",
                "squareid": "$PYTHON[int(mainmenuid) * 1000 + 421]",
                "squaregroupid": "$PYTHON[int(mainmenuid) * 1000 + 422]",
                "posterid": "$PYTHON[int(mainmenuid) * 1000 + 431]",
                "postergroupid": "$PYTHON[int(mainmenuid) * 1000 + 432]",
                "grouplistid": "$PYTHON[int(mainmenuid) * 1000 + 400]",
                "topid": "$PYTHON[int(mainmenuid) * 1000 + 601]",
                "toplandscapeid": "$PYTHON[int(mainmenuid) * 1000 + 411]",
                "topsquareid": "$PYTHON[int(mainmenuid) * 1000 + 421]",
                "topposterid": "$PYTHON[int(mainmenuid) * 1000 + 431]",
                "submenuid": "302",
                "altvisible": "Control.HasFocus(300) | Control.HasFocus(302)",
                "include_condition": "String.IsEmpty(Skin.String(Home.LayoutStyle))",
                "content_landscape": "$INFO[Window.Property(Item.$PYTHON[int(mainmenuid) * 1000 + 302].SpecialFolderPath)]",
                "content_square": "$INFO[Window.Property(Item.$PYTHON[int(mainmenuid) * 1000 + 302].SpecialFolderPath)]",
                "content_poster": "$INFO[Window.Property(Item.$PYTHON[int(mainmenuid) * 1000 + 302].SpecialFolderPath)]",
                "label": "$INFO[Window.Property(Item.$PYTHON[int(mainmenuid) * 1000 + 302].SpecialFolderLabel)]",
                "sort_label" :"$LOCALIZE[571]",
                "enabled": "!String.IsEmpty(Window.Property(Item.$PYTHON[int(mainmenuid) * 1000 + 302].SpecialFolderPath)) + !String.IsEmpty(Container(302).ListItem.Property(useWidgetList))",
                "prefix": "Item.$PYTHON[int(mainmenuid) * 1000 + 302]."
            },
            "parts_endrowselector": {
                "template": "skinvariables-generator-parts-endrowselector.xmltemplate",
                "id": "$PYTHON[int(mainmenuid) * 1000 + 419]",
                "groupid": "$PYTHON[int(mainmenuid) * 1000 + 429]",
                "grouplistid": "$PYTHON[int(mainmenuid) * 1000 + 400]",
                "topid": "$PYTHON[int(mainmenuid) * 1000 + 601]",
                "toplandscapeid": "$PYTHON[int(mainmenuid) * 1000 + 411]",
                "topsquareid": "$PYTHON[int(mainmenuid) * 1000 + 421]",
                "topposterid": "$PYTHON[int(mainmenuid) * 1000 + 431]",
                "content": "<include>skinshortcuts-submenu-1</include>",
                "include_condition": "Skin.HasSetting(Hubs.WidgetSelector)"
            },
            "parts_widgets": {
                "template": "skinvariables-generator-parts-widgets.xmltemplate",
                "id": "$PYTHON[int(mainmenuid) * 1000 + 600 + int(id)]",
                "groupid": "$PYTHON[int(mainmenuid) * 1000 + 700 + int(id)]",
                "grouplistid": "$PYTHON[int(mainmenuid) * 1000 + 400]",
                "landscapeid": "$PYTHON[int(mainmenuid) * 1000 + 411]",
                "squareid": "$PYTHON[int(mainmenuid) * 1000 + 421]",
                "topid": "$PYTHON[int(mainmenuid) * 1000 + 601]",
                "refocus": "SetFocus(393)",
                "visible": "[String.IsEqual(Container(300).ListItem.Property(submenuVisibility),$SKINSHORTCUTS[submenuVisibility]) + [String.IsEqual(Container($PYTHON[int(mainmenuid) * 1000 + 419]).ListItem.Label,$SKINSHORTCUTS[widgetName]) | !Skin.HasSetting(Hubs.WidgetSelector)]]",
                "altvisible": "[$SKINSHORTCUTS[widgetAltVisible]]",
                "include_condition": "$SKINSHORTCUTS[isVisible]"
            }
        },
